{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        //"no-unused-var": "off",
        "semi": ["error", "always"],
        "indent": ["error", 4],
        "quotes": ["error", "double"],
        "no-eval": "off",
        "camelcase": ["error", { "properties": "always" }]
    }
}
